# bootstrap
if __name__ == "__main__":
    # access the package
    import altar
    # build an instance of the default app
    app = altar.shells.altar(name="altar.plexus")
    # invoke the main entry point
    status = app.run()
    # share
    raise SystemExit(status)

# end of file
